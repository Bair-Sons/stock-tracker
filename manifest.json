{
  "manifest_version": 3,
  "name": "Stock Prices",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://*/", "https://*/"],
      "js": ["core.js"]
    }
  ],
  "action": {
    "default_popup": "base.html",
    "default_icon": "icon.png"
  },
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "settings": "settings.html"
  }
}